<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ro.lbi.sqliggybank.client.view.dashboard.TransactionsController">
    <Label layoutX="42.0" layoutY="33.0" prefHeight="53.0" prefWidth="156.0" text="Transactions">
        <font>
            <Font name="Arial Bold" size="24.0"/>
        </font>
    </Label>
    <Label alignment="CENTER_RIGHT" layoutX="56.0" layoutY="107.0" prefHeight="50.0" prefWidth="100.0" text="Amount:"/>
    <Label alignment="CENTER_RIGHT" layoutX="56.0" layoutY="149.0" prefHeight="50.0" prefWidth="100.0"
